@model IList<AppTemplate.Domain.User>

@{
}
@Html.ActionLink("Register New User", "Register", "Users", new{@class="btn"})
    <table class="table table-striped">
      <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
          <th>Role</th>
            <th>
                Created
            </th>
            <th>Last Login</th>
        </tr>

      </thead>
    <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.ActionLink(item.Username, "Details", new { id=item.Id })
            </td>
            <td>
                @item.Email
            </td>
            <td>
                @Html.DisplayFor(itemModel => item.UserRole)
            </td>
            <td>@item.DateCreated
            </td>
          <td>@item.DateLastLogin</td>
        </tr>
    }
    </tbody>
    </table>
